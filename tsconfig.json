{
  "types": [
    "@types/gtag.js"
  ],
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "useUnknownInCatchVariables": false,
    "baseUrl": "."
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "custom.d.ts",
    ".module.css"
  ],
  "exclude": [
    "node_modules"
  ]
}